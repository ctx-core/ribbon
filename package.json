{
	"name": "@ctx-core/ribbon",
	"version": "10.0.83",
	"description": "ctx-core ribbon",
	"keywords": [
		"ctx-core",
		"ribbon"
	],
	"homepage": "https://github.com/ctx-core/ribbon#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/ribbon/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ribbon/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.81",
		"@ctx-core/sass": "workspace:^12.1.6",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"svelte": "3.44.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}