{
	"name": "@ctx-core/ribbon",
	"version": "10.0.140",
	"description": "ctx-core ribbon",
	"keywords": [
		"ctx-core",
		"ribbon"
	],
	"homepage": "https://github.com/ctx-core/ribbon#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/ribbon/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ribbon.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"devDependencies": {
		"@ctx-core/sass": "^12.1.74",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.23",
		"rimraf": "^3.0.2",
		"svelte": "^3.55.0",
		"svelte-preprocess": "^5.0.0",
		"svelte2tsx": "^0.5.23",
		"tslib": "^2.4.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"sideEffects": false
}